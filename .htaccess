RewriteRule ^service-detail-(.*)\.html$ ?hal=service-detail&id=$1 [L]

RewriteRule ^contact.html$ ?hal=contact [L]


RewriteRule ^login.html$ ?hal=login [L]
RewriteRule ^member_area.html$ ?hal=member_area [L]


